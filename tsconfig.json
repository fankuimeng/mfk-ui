{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "ESNext",
    "allowJs": true,
    "allowSyntheticDefaultImports": false,
    "useDefineForClassFields": false,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "noEmit": true
  }
}
